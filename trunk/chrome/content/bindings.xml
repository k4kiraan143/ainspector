<?xml version="1.0"?>

<bindings xmlns="http://www.mozilla.org/xbl"
	xmlns:xbl="http://www.mozilla.org/xbl"
	xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

	<binding id="startup">
		<implementation>
			<constructor><![CDATA[
				AINSPECTOR.firefox.startup(false);
			]]></constructor>
			<destructor><![CDATA[
				AINSPECTOR.firefox.shutdown();
			]]></destructor>
		</implementation>
	</binding>

	<binding id="windowStartup">
		<implementation>
			<constructor><![CDATA[
				AINSPECTOR.firefox.startup(true);
			]]></constructor>
			<destructor><![CDATA[
				AINSPECTOR.firefox.shutdown();
			]]></destructor>
		</implementation>
	</binding>

</bindings>
